<defaults>
	<text font="Fonts/AdonisC" />
	<button font="Fonts/AdonisC" />
</defaults>
<button
	id="help"
	onClick="Global/showHelp"
	width="400"
	height="400"
	position="540 -52 -22"
	rotation="0 0 180"
	scale="0.115 0.115 1"
	color="clear"
	text=""/>
<panel
	interactable="false"
	id="results"
	width="170"
	height="160"
	position="430 0 -22"
	rotation="0 0 180"
    onMouseEnter="hoverResultsEnter"
    onMouseExit="hoverResultsExit">
    <Panel id="results_default" active="true">
		<text
			id="success"
			width="640"
			height="640"
			position="0 -25 0"
			scale="0.35 0.35 1"
			color="#a29c90"
			font="Fonts/AdonisC"
			fontSize="1000"
			text="?"/>
    </Panel>
    <Panel id="results_hover" active="false">
		<Text text="успіхів" fontSize="30" color="#a29c90" position="-40 5 0" />
		<Text id="success_additional" text="?" fontSize="90" color="#a29c90" position="35 20 0" />
		<Text text="перекидів всього" fontSize="16" color="#a29c90" position="-40 -30 0" />
		<Text id="reroll" text="0" fontSize="22" color="#a29c90"  position="40 -30 0" />
		<Text text="перекинуто кубів" fontSize="16" color="#a29c90" position="-40 -60 0" />
		<Text id="rerolled_dices" text="0" fontSize="22" color="#a29c90" position="40 -60 0" />
    </Panel>
</panel>
